a=int(input("a:"))
b=int(input("b:"))
operation=input("add/subract/mul/div:")
if(operation=="add"):
    print(a+b)
elif(operation=="subract"):  
    print(a-b)
elif(operation=="mul"):  
    print(a*b)
elif(operation=="div"):  
    print(a/b)
else:
    print("Invalid")
---------------------------------------------------------------------------------------------------------------------------------------------------------------------
