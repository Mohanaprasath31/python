----------------------------------------------------------------------------------------------------------------------------------------------------------------------
a=int(input("a:"))
b=int(input("b:"))
operation=input("add/subract/mul/div:")
if(operation=="add"):
    print(a+b)
elif(operation=="subract"):  
    print(a-b)
elif(operation=="mul"):  
    print(a*b)
elif(operation=="div"):  
    print(a/b)
else:
    print("Invalid")
---------------------------------------------------------------------------------------------------------------------------------------------------------------------
count=0
for i in range(1,11):
    if(i%2==0):
        count+=1
print(count)
---------------------------------------------------------------------------------------------------------------------------------------------------------------------
a=[]
print("Enter 10 numbers")
for i in range(1,11):
    num=int(input("enter number"+str(i)))
    a.append(num)
print(a)
sum=0
for i in a:
    sum=sum+i
print("sum=",sum)
